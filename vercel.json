{
  "framework": null,
  "builds": [
    {
      "src": "messaging_app/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize":  "15mb", "runtime":  "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "messaging_app/wsgi.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS, DELETE, PUT"
      }
    },
    {
      "src": "/(.*)",
      "dest": "messaging_app/wsgi.py"
    }
  ]
}